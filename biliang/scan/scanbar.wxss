/*---------- radar start ----------*/
.radar {
  width: 100%;
  height: 330px;
  position: relative;
}
.circle_end {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin:auto;
  width: 300px;
  height: 300px;
  box-sizing: border-box;
  border-radius:50%;
  border: 4px solid rgba(255, 255, 255, 0.5);
}
.circle {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin:auto;
  width: 300px;
  height: 300px;
  border-radius:50%;
}
.gradient_circle {
  position: absolute;
  top: 0;
  left: 0;
}
.scan {
  width: 150px;
  height: 150px;
  background:linear-gradient(0deg,rgba(40, 168, 252, 0.0),rgba(255, 255, 255, 0.45));
  border-radius:100% 0 0 0;
  z-index:999;
  position: relative;

}
.gradient_dot {
  position: absolute;
  top: -14px;
  right: -14px;
}
.cursor {
  width: 2px;
  height: 100%;
  background: linear-gradient(0deg,rgba(40, 168, 252, 0.0),rgba(255, 255, 255, 0.45));
  position: absolute;
  top: 0;
  right: 0;
}

